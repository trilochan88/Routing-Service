{
  "request": {
    "method": "POST",
    "url": "/update-points",
    "headers": {
      "Content-Type": {
        "equalTo": "application/json"
      }
    },
    "bodyPatterns": [
      {
        "equalToJson": "{\"game\":\"Mobile Legends\", \"gamerID\":\"GYUTDTE\", \"points\":20}",
        "ignoreExtraElements": true
      }
    ]
  },
  "response": {
    "status": 200,
    "jsonBody": {
      "game": "Mobile Legends",
      "gamerID": "GYUTDTE",
      "points": 20
    },
    "headers": {
      "Content-Type": "application/json"
    }
  }
}
